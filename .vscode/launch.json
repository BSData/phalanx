{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/.build/bin/Phalanx.PlaygroundTool/Debug/net6.0/Phalanx.PlaygroundTool.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Phalanx.PlaygroundTool",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}